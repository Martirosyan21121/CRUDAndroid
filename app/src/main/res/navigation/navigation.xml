<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/firstPageFragment2">
    <fragment
        android:id="@+id/firstPageFragment2"
        android:name="am.example.crudapplication.fragments.FirstPageFragment"
        android:label="fragment_first_page"
        tools:layout="@layout/fragment_first_page" >
        <action
            android:id="@+id/action_firstPageFragment2_to_addUserFragment2"
            app:destination="@id/addUserFragment2" />
        <action
            android:id="@+id/action_firstPageFragment2_to_updateUserFragment2"
            app:destination="@id/updateUserFragment2" />
    </fragment>
    <fragment
        android:id="@+id/addUserFragment2"
        android:name="am.example.crudapplication.fragments.AddUserFragment"
        android:label="add_user_fragment"
        tools:layout="@layout/add_user_fragment" >
        <action
            android:id="@+id/action_addUserFragment2_to_firstPageFragment2"
            app:destination="@id/firstPageFragment2" />
    </fragment>
    <fragment
        android:id="@+id/updateUserFragment2"
        android:name="am.example.crudapplication.fragments.UpdateUserFragment"
        android:label="user_update_fragment"
        tools:layout="@layout/user_update_fragment" />
</navigation>